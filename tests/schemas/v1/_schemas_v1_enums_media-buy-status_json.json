{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/schemas/v1/enums/media-buy-status.json",
  "title": "Media Buy Status",
  "description": "Status of a media buy",
  "type": "string",
  "enum": [
    "pending_activation",
    "active",
    "paused",
    "completed"
  ],
  "enumDescriptions": {
